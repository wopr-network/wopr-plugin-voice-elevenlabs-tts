{
  "author": "",
  "dependencies": {
    "@wopr-network/plugin-types": "^0.1.0",
    "node-fetch": "^3.3.2"
  },
  "description": "ElevenLabs TTS provider for WOPR voice plugin system",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.2.3",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "wopr",
    "tts",
    "elevenlabs",
    "voice",
    "speech-synthesis"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@wopr-network/wopr-plugin-voice-elevenlabs-tts",
  "peerDependencies": {
    "wopr": "*"
  },
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "release": {
    "extends": "@wopr-network/semantic-release-config"
  },
  "wopr": {
    "plugin": {
      "category": "voice",
      "description": "ElevenLabs TTS (hosted API)",
      "name": "voice-elevenlabs-tts",
      "type": "tts"
    }
  }
}
